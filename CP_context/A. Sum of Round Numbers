import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int t=sc.nextInt();
        while(t-->0){
            int n=sc.nextInt();
            int x=0;
            int count=0;
            ArrayList<Integer>a=new ArrayList<>();
            while(n>0 && n<=10000){
                if(n>=1000){
                    x=n/1000;
                    a.add(x *1000);
                    n=n%1000;
                    
                }
                else if(n>=100){
                    x=n/100;
                    a.add(x*100);
                    n=n%100;
                
                }
                else if(n>=10){
                    x=n/10;
                    a.add(x*10);
                    n=n%10;
                }
                else{
                    a.add(n);
                    n=0;
                }
                count++;
            }
            System.out.println(count);
            Collections.sort(a);
            for(int i=0;i<a.size();i++){
                System.out.print(a.get(i)+" ");
            }
            System.out.println();
            
        }
    }
}
