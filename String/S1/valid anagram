class Solution {
    public boolean isAnagram(String s, String t) {
        // for(int i=0; i<s.length(); i++){
        //     for(int j=0; j<t.length(); j++){
        //         if(s.charAt(i)==t.charAt(j)) return true;
        //     }

        // }
        // return false;


        char[] c1=s.toCharArray();
        char[] c2=t.toCharArray();

        Arrays.sort(c1);
        Arrays.sort(c2);
        return Arrays.equals(c1,c2);
    }
}
